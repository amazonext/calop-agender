{
  "expo": {
    "name": "Calop Agender",
    "slug": "calop-agender",
    "splash": {
      "image": "./src/assets/images/icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#C0933E"
    },
    "assetBundlePatterns": [
      "./src/assets/**"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/assets/images/icon.png",
    "web": {
      "favicon": "./src/assets/images/icon.png"
    },
    "newArchEnabled": true,
    "android": {
      "package": "com.theheapsters.calopagender",
      "permissions": [
        "READ_CALENDAR",
        "WRITE_CALENDAR"
      ]
    },
    "ios": {
      "bundleIdentifier": "com.theheapsters.calopagender",
      "infoPlist": {
        "NSCalendarsUsageDescription": "Este app precisa acessar seu calend√°rio para adicionar eventos."
      }
    },
    "extra": {
      "eas": {
        "projectId": "84d68c36-0d29-430d-a19f-6ca42cb830f3"
      }
    },
    "androidNavigationBar": {
      "barStyle": "dark-content",
      "backgroundColor": "#C0933E"
    },
    "owner": "the-heapsters",
    "plugins": [
      "expo-router"
    ]
  }
}
